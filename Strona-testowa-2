<!-- Swiper JS -->
<script src="https://cdn.jsdelivr.net/npm/swiper/swiper-bundle.min.js"></script>

<!-- Initialize Swiper -->
<script>
    var swiper = new Swiper(".mySwiper", {
        loop: true,
        autoplay: {
            delay: 5500,
            disableOnInteraction: false,
        },
        navigation: {
            nextEl: ".button-next",
            prevEl: ".button-prev",
        },
        pagination: {
            el: ".swiper-pagination",
        },
    });
</script>

<!-- Initialize Swiper2 -->
<script>
    var swiper = new Swiper(".mySwiper2", {
        slidesPerView: 6,
        spaceBetween: 30,
        loop: true,
        navigation: {
            nextEl: ".button-next-small",
            prevEl: ".button-prev-small",
        },
        autoplay: {
            delay: 1500,
            disableOnInteraction: false,
        },
        pagination: {
            clickable: true,
        },
    });
</script>

<!-- Initialize Swiper3 -->
<script>
    var swiper = new Swiper(".mySwiper3", {
        slidesPerView: 4,
        spaceBetween: 0,
        loop: true,
        navigation: {
            nextEl: ".button-next-small2",
            prevEl: ".button-prev-small2",
        },
        autoplay: {
            delay: 1500,
            disableOnInteraction: false,
        },
    });
</script>

<!-- Initialize Swiper4 -->
<script>
    var swiper = new Swiper(".mySwiper4", {
        slidesPerView: 8,
        spaceBetween: 0,
        loop: true,
        autoplay: {
            delay: 2500,
            disableOnInteraction: false,
        },
    });
</script>

<!-- Initialize Swiper5 -->
<script>
    var swiper = new Swiper(".mySwiper5", {
        slidesPerView: 3,
        spaceBetween: 30,
        loop: true,
        navigation: {
            nextEl: ".button-next-small3",
            prevEl: ".button-prev-small3",
        },
        autoplay: {
            delay: 1500,
            disableOnInteraction: false,
        },
        pagination: {
            clickable: true,
        },
    });
</script>

<!--sliding menu-->
<script>
    const navbar = document.getElementById('navbar')
    const onScroll = () => {
        const scroll = document.documentElement.scrollTop
        if (scroll > 50) {
            navbar.classList.add("active");
        } else {
            navbar.classList.remove("active")
        }

    }
    window.addEventListener('scroll', onScroll)
</script>

<script src="js/bootstrap.min.js"></script>

